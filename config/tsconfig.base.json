{
    "$schema":"https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "downlevelIteration": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "node"
    }
}